<?xml version="1.0" encoding="UTF8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!--    -->
    <bean name="user" class="com.why.start.spring.pojo.User"/>

    <bean name="user2" class="com.why.start.spring.pojo.User">
        <constructor-arg name="age" value="10"/>
        <constructor-arg name="name" value="why-cons"/>
    </bean>

    <bean name="user3" class="com.why.start.spring.pojo.User">
        <property name="name" value="why-pro"/>
        <property name="age" value="12"/>
    </bean>
</beans>